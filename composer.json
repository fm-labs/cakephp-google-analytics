{
    "name": "fm-labs/cakephp-google-analytics",
    "description": "Google Analytics plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "cakephp/cakephp": "^4.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "GoogleAnalytics\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GoogleAnalytics\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
